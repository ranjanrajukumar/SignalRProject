{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "signalr.js@2.4.3",
      "destination": "wwwroot/lib/signalr/signalr.js/"
    },
    {
      "provider": "cdnjs",
      "library": "microsoft-signalr@7.0.7",
      "destination": "wwwroot/lib/signalr/microsoft-signalr/"
    }
  ]
}